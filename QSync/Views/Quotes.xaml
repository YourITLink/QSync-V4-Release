<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
        xmlns:system="clr-namespace:System;assembly=mscorlib"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QSync.Views"
        xmlns:localVMs="clr-namespace:QSync.ViewModels"
        xmlns:localModels="clr-namespace:QSync.Models"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
        xmlns:md="https://github.com/fantasticfiasco/mvvm-dialogs"
        xmlns:extToolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit/extended"
        xmlns:telerik="http://schemas.telerik.com/2008/xaml/presentation" xmlns:QSync="clr-namespace:QSync" xmlns:syncfusion="http://schemas.syncfusion.com/wpf" x:Class="QSync.Views.Quotes"
        TextElement.Foreground="{DynamicResource WordBlueBrush}"
        Background="{DynamicResource MaterialDesignPaper}"
        TextElement.FontWeight="Medium"
        mc:Ignorable="d"
        Height="1000" Width="1920" ScrollViewer.CanContentScroll="True"
        WindowStartupLocation="CenterOwner" WindowState="Maximized" Loaded="Window_Loaded">
    <Window.Resources>
        <CollectionViewSource x:Key="quoteViewSource" d:DesignSource="{d:DesignInstance {x:Type QSync:quote}, CreateList=True}"/>
        <CollectionViewSource x:Key="quotequoteitemsViewSource" Source="{Binding quoteitems, Source={StaticResource quoteViewSource}}"/>
        <RoutedUICommand x:Key="FirstCommand" Text="First"/>
        <RoutedUICommand x:Key="LastCommand" Text="Last"/>
        <RoutedUICommand x:Key="NextCommand" Text="Next"/>
        <RoutedUICommand x:Key="PreviousCommand" Text="Previous"/>
        <RoutedUICommand x:Key="DeleteCustomerCommand" Text="Delete Customer"/>
        <RoutedUICommand x:Key="DeleteOrderCommand" Text="Delete Order"/>
        <RoutedUICommand x:Key="UpdateCommand" Text="Update"/>
        <RoutedUICommand x:Key="AddCommand" Text="Add"/>
        <RoutedUICommand x:Key="CancelCommand" Text="Cancel"/>
        <RoutedUICommand x:Key="CommitInvoice" Text="Commit"/>
    </Window.Resources>
    <TextElement.FontSize>
        <StaticResource ResourceKey="FontSizeRegular"/>
    </TextElement.FontSize>
    <Window.FontFamily>
        <StaticResource ResourceKey="App"/>
    </Window.FontFamily>
    <Window.CommandBindings>
        <CommandBinding Command="{StaticResource FirstCommand}" Executed="FirstCommandHandler"/>
        <CommandBinding Command="{StaticResource LastCommand}" Executed="LastCommandHandler"/>
        <CommandBinding Command="{StaticResource NextCommand}" Executed="NextCommandHandler"/>
        <CommandBinding Command="{StaticResource PreviousCommand}" Executed="PreviousCommandHandler"/>
        <CommandBinding Command="{StaticResource DeleteCustomerCommand}" Executed="DeleteCustomerCommandHandler"/>
        <CommandBinding Command="{StaticResource DeleteOrderCommand}" Executed="DeleteOrderCommandHandler"/>
        <CommandBinding Command="{StaticResource UpdateCommand}" Executed="UpdateCommandHandler"/>
        <CommandBinding Command="{StaticResource AddCommand}" Executed="AddCommandHandler"/>
        <CommandBinding Command="{StaticResource CancelCommand}" Executed="CancelCommandHandler"/>
    </Window.CommandBindings>
    <DockPanel>
        <!-- File Menu -->
        <Menu DockPanel.Dock="Top" Margin="0,0,0.4,0">
            <MenuItem x:Name="fileMenu" Header="File" FontSize="18">
                <MenuItem Header="New Quote" Click="OpenQuotes_Click" InputGestureText="Ctrl+Q"/>
                <MenuItem Header="New Invoice" Click="OpenInvoices_Click" />
                <MenuItem Header="Enter Service" Click="OpenServices_Click" />
                <Separator />
                <MenuItem Header="Main Menu" Click="MainMenu_Click" />
                <MenuItem Header="Exit QSync" Click="CloseQSync_Click" />
            </MenuItem>
            <MenuItem x:Name="fileQuotes" Header="Quotes" FontSize="18" Visibility="Collapsed">
                <MenuItem Header="Create a Quote" Click="OpenQuotes_Click" InputGestureText="Ctrl+Q" />
                <MenuItem Header="Search for a Quote" Click="SearchQuotes_Click" InputGestureText="Ctrl+F" />
                <MenuItem Header="QSync Email System" Click="OpenQuotes_Click" InputGestureText="Ctrl+E" />
            </MenuItem>
            <MenuItem x:Name="fileService" Header="Service" FontSize="18" Visibility="Visible">
                <MenuItem Header="View Customers" Click="OpenServices_Click"  />
                <MenuItem Header="Return DSR's" Click="OpenServices_Click"  />
                <MenuItem Header="Create DSR's" Click="OpenServices_Click" />
            </MenuItem>
            <MenuItem x:Name="fileInvoices" Header="Invoices" FontSize="18" Visibility="Collapsed">
                <MenuItem Header="New Dummy Invoice" Click="OpenInvoices_Click" />
                <MenuItem Header="Load Invoices" Click="OpenInvoices_Click" />
                <MenuItem Header="Find a Dummy Invoice" Click="OpenInvoices_Click" />
            </MenuItem>
            <MenuItem x:Name="fileShutter" Header="Shutter Profiller" FontSize="18" Visibility="Collapsed">
                <MenuItem Header="Build a Door" />
                <MenuItem Header="Lookup Previous" />
            </MenuItem>
            <MenuItem x:Name="fileCustom" Header="Customise" FontSize="18">
                <MenuItem Header="My Settings" Click="UserSettings_Click" />
            </MenuItem>
            <MenuItem x:Name="fileManager" Header="Manager Menu" FontSize="18" Visibility="Collapsed">
                <MenuItem Header="QSync Maintenance"  />
                <MenuItem Header="Reset User Password"  />
                <MenuItem Header="Access Levels"  />
                <MenuItem Header="Quote Conditions" />
            </MenuItem>
            <MenuItem x:Name="fileDev" Header="Your IT Link" FontSize="18" Visibility="Collapsed">
                <MenuItem Header="Email Test" Click="dev1_Click" />
                <MenuItem Header="Build Changes" Click="dev2_Click" />
                <MenuItem Header="Duplicate" Click="quoteDuplicate_Click"/>
                <MenuItem Header="Dev 4" Click="dev5_Click"  />
                <MenuItem Header="Dev 5" Click="dev5_Click"  />
            </MenuItem>
            <MenuItem Header="About" Click="about_Click" FontSize="18"/>
        </Menu>


            <StackPanel VerticalAlignment="Top" DockPanel.Dock="Top" Height="70">
                <materialDesign:DrawerHost Height="69">
                    <materialDesign:ColorZone Padding="16" materialDesign:ShadowAssist.ShadowDepth="Depth2" Mode="PrimaryDark" DockPanel.Dock="Top" Background="#FF263B8F">
                        <Grid>
                            <TextBlock x:Name="lblLocation" Foreground="GhostWhite" Text="USER LOCATION" FontSize="22" HorizontalAlignment="Left" DockPanel.Dock="Left"/>
                            <TextBlock x:Name="title" FontSize="22" Margin="0" Foreground="#FFFDD835" HorizontalAlignment="Center"><Run Text="Quotations - QSync - Able Door Services"/></TextBlock>
                            <TextBlock x:Name="QID" HorizontalAlignment="Right" Height="30" FontSize="24" Foreground="#FFFDD835" FontWeight="Bold" DockPanel.Dock="Right"><Run Text="{Binding Text, ElementName=intquoteid}"/><Run Text="{Binding Text, ElementName=quoteid}"/></TextBlock>
                        </Grid>
                    </materialDesign:ColorZone>
                </materialDesign:DrawerHost>
            </StackPanel>

            

            <!-- Footer placeholder -->
            <Frame DockPanel.Dock="Bottom" x:Name="Footer" />

            <!-- Command Buttons -->
            <Grid Margin="0,-0.333">
                <StackPanel x:Name="quoteBtn" Orientation="Vertical" Margin="0,76,0,0" HorizontalAlignment="Left" Visibility="Visible">
                    <Button x:Name="btnFirst" Content="|◄ Oldest" Grid.Row="2" FontSize="18" Grid.Column="0" Command="{StaticResource FirstCommand}" Style="{StaticResource MaterialDesignRaisedButton}" Width="130" ToolTip="Load OLDEST Quote" Background="#FF263B8F" BorderBrush="#FF263B8F" Opacity="1" Foreground="#FFFDD835" Margin="10,5" Height="60"/>
                    <Button x:Name="btnPrev" Content="◄ Prev" Grid.Row="2" FontSize="18" Grid.Column="0" Command="{StaticResource PreviousCommand}" Style="{StaticResource MaterialDesignRaisedButton}" Width="130" ToolTip="Load Previous Quote" Background="#FF263B8F" BorderBrush="#FF263B8F" Opacity="1" Foreground="#FFFDD835" Margin="10,5" Height="60"/>
                    <Button x:Name="btnNext" Content="Fwd ►" Grid.Row="2" FontSize="18" Grid.Column="11" Command="{StaticResource NextCommand}" Style="{StaticResource MaterialDesignRaisedButton}" Width="130" ToolTip="Load Next Quote" Opacity="1" Background="#FF263B8F" BorderBrush="#FF263B8F" Foreground="#FFFDD835" Margin="10,5" Height="60"/>
                    <Button x:Name="btnLast" Content="Newest ►|" Grid.Row="2" FontSize="18" Grid.Column="12" Command="{StaticResource LastCommand}" Style="{StaticResource MaterialDesignRaisedButton}" Width="130" ToolTip="Load Newest Quote" BorderBrush="#FF263B8F" Foreground="#FFFDD835" Opacity="1" Background="#FF263B8F" Margin="10,5" Height="60"/>
                    <Button x:Name="btnAdd" Content="New Quote" Grid.Row="2" Grid.Column="5" Command="{StaticResource AddCommand}" Style="{StaticResource MaterialDesignRaisedButton}" Width="130" ToolTip="Create a NEW Quote" Opacity="1" Background="#FF263B8F" BorderBrush="#FF263B8F" Foreground="#FFFDD835" Margin="10,5" FontSize="18" Height="60"/>
                    <Button x:Name="btnUpdate" Content="Save" Grid.Row="2" Grid.Column="3" Command="{StaticResource UpdateCommand}" Style="{StaticResource MaterialDesignRaisedButton}" Width="130" ToolTip="Save Changes to Quote" Opacity="1" Background="#FF263B8F" BorderBrush="#FF263B8F" Foreground="#FFFDD835" Margin="10,5" FontSize="18" Height="60"/>
                    <Button x:Name="btnRefresh" Content="Refresh" Visibility="Collapsed" Grid.Row="2" Grid.Column="7" Command="{StaticResource DeleteCustomerCommand}" Style="{StaticResource MaterialDesignRaisedButton}" Width="130" ToolTip="Refresh Current Data" Opacity="1" Background="#FF263B8F" BorderBrush="#FF263B8F" Foreground="#FFFDD835" Margin="10,5" FontSize="18" Height="60"/>
                    <Button x:Name="btnPreview" Content="Preview" Grid.Row="2" Grid.Column="7" Click="BtnPreview_Click" Style="{StaticResource MaterialDesignRaisedButton}" Width="130" ToolTip="Preview, Print and Email Quote" Opacity="1" Background="#FFFDD835" BorderBrush="#FFFDD835" Foreground="#FF263B8F" Margin="10,5" FontSize="18" Height="60"/>
                    <Button x:Name="btnFindQuote" Content="Search" Grid.Row="2" Grid.Column="7" Click="LoadSearchQuotes_click" Style="{StaticResource MaterialDesignRaisedButton}" Width="130" ToolTip="Search for Existing Quote" Opacity="1" Background="#FF263B8F" BorderBrush="#FF263B8F" Foreground="#FFFDD835" Margin="10,5" FontSize="18" Height="60"/>
                </StackPanel>

                <!-- Search Tab Command Buttons -->
                <Grid Margin="0,-13,0.333,-0.001">
                    <StackPanel x:Name="quoteSearchBtn" Visibility="Collapsed" Orientation="Vertical" Margin="0,76,0,0" HorizontalAlignment="Left">
                        <Button x:Name="btnLoadQuote" Content="Load Quote" Grid.Row="2" FontSize="18" Grid.Column="0" Click="LoadSelectedQuoteView_click" Style="{StaticResource MaterialDesignRaisedButton}" Width="130" ToolTip="Load Selected Quote" Background="#FF263B8F" BorderBrush="#FF263B8F" Opacity="1" Foreground="#FFFDD835" Margin="10,5" Height="60"/>
                        <Button x:Name="btnDuplicate" Content="Duplicate" Grid.Row="2" Grid.Column="3" Click="quoteDuplicate_Click" Style="{StaticResource MaterialDesignRaisedButton}" Width="130" ToolTip="Duplicate Quote" Opacity="1" Background="#FF263B8F" BorderBrush="#FF263B8F" Foreground="#FFFDD835" Margin="10,5" FontSize="18" Height="60"/>
                        <Button x:Name="btnViewQuote" Visibility="Collapsed" Content="View Report" Grid.Row="2" FontSize="18" Grid.Column="11" Command="{StaticResource NextCommand}" Style="{StaticResource MaterialDesignRaisedButton}" Width="130" ToolTip="View Current Quote" Opacity="1" Background="#FF263B8F" BorderBrush="#FF263B8F" Foreground="#FFFDD835" Margin="10,5" Height="60"/>
                        <Button x:Name="btnPrintQuote" Content="Print Quote" Grid.Row="2" FontSize="18" Grid.Column="12" Click="BtnPreview_Click" Style="{StaticResource MaterialDesignRaisedButton}" Width="130" ToolTip="Print Quote" Background="#FFFDD835" BorderBrush="#FFFDD835" Foreground="#FF263B8F" Opacity="1" Margin="10,5" Height="60"/>
                        <Button x:Name="btnEmailQuote" Visibility="Collapsed" Content="Email Quote" Grid.Row="2" Grid.Column="5" Command="{StaticResource AddCommand}" Style="{StaticResource MaterialDesignRaisedButton}" Width="130" ToolTip="Email Quote" Opacity="1" Background="#FF263B8F" BorderBrush="#FF263B8F" Foreground="#FFFDD835" Margin="10,5" FontSize="18" Height="60"/>
                        <Button x:Name="btnSaveCopy" Visibility="Collapsed" Content="Save Copy" Grid.Row="2" Grid.Column="3" Command="{StaticResource UpdateCommand}" Style="{StaticResource MaterialDesignRaisedButton}" Width="130" ToolTip="Save Changes" Opacity="1" Background="#FF263B8F" BorderBrush="#FF263B8F" Foreground="#FFFDD835" Margin="10,5" FontSize="18" Height="60"/>
                        <Button x:Name="btnPreviewSearch" Content="Preview" Grid.Row="2" Grid.Column="7" Click="BtnPreview_Click" Style="{StaticResource MaterialDesignRaisedButton}" Width="130" ToolTip="Save and Preview Current Quote" Opacity="1" Background="#FFFDD835" BorderBrush="#FFFDD835" Foreground="#FF263B8F" Margin="10,5" FontSize="18" Height="60"/>
                        <Button x:Name="btnLoadedUpdate" Content="Save" Grid.Row="2" Grid.Column="3" Command="{StaticResource UpdateCommand}" Style="{StaticResource MaterialDesignRaisedButton}" Width="130" ToolTip="Save Changes to quote Session" Opacity="1" Background="#FF263B8F" BorderBrush="#FF263B8F" Foreground="#FFFDD835" Margin="10,5" FontSize="18" Height="60"/>
                        <Button x:Name="btncSearchCancel" Content="Cancel" Grid.Row="2" FontSize="18" Grid.Column="0" Click="CancelSearchQuotes_click" Style="{StaticResource MaterialDesignRaisedButton}" Width="130" ToolTip="Cancel Search" Background="#FF263B8F" BorderBrush="#FF263B8F" Opacity="1" Foreground="#FFFDD835" Margin="10,5" Height="60"/>
                    </StackPanel>

                    <!-- Add Quote Tab Command Buttons -->
                    <Grid Margin="0,-13,0.333,-0.001">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="1829*"/>
                            <ColumnDefinition Width="15*"/>
                            <ColumnDefinition Width="29*"/>
                            <ColumnDefinition Width="40*"/>
                        </Grid.ColumnDefinitions>
                        <StackPanel x:Name="addQuoteBtn" Visibility="Collapsed" Orientation="Vertical" Margin="0,76,0,-0.332" HorizontalAlignment="Left">
                            <Button x:Name="btnCommit" Content="Commit" Grid.Row="2" Click="AddRecord_Click" Grid.Column="8" Style="{StaticResource MaterialDesignRaisedButton}" Width="130" ToolTip="Confirm Quote and Continue" Opacity="1" Background="#FF263B8F" BorderBrush="#FF263B8F" Foreground="#FFFDD835" Margin="10,5" FontSize="18" Height="60"/>
                            <Button x:Name="btnAddCancel" Content="Cancel" Grid.Row="2" FontSize="18" Grid.Column="0" Click="CancelAddQuotes_click" Style="{StaticResource MaterialDesignRaisedButton}" Width="130" ToolTip="Cancel New Quote" Background="#FF263B8F" BorderBrush="#FF263B8F" Opacity="1" Foreground="#FFFDD835" Margin="10,5" Height="60"/>
                        </StackPanel>

                        <!-- Duplicate Quote Tab Command Buttons -->
                        <Grid Margin="0,-13,0.333,-0.001">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="1829*"/>
                                <ColumnDefinition Width="15*"/>
                                <ColumnDefinition Width="29*"/>
                                <ColumnDefinition Width="40*"/>
                            </Grid.ColumnDefinitions>
                            <StackPanel x:Name="duplicateQuoteBtn" Visibility="Collapsed" Orientation="Vertical" Margin="0,76,0,-0.332" HorizontalAlignment="Left">
                                <Button x:Name="btnDupCommit" Content="Commit" Grid.Row="2" Click="DuplicateCommit_Click" Grid.Column="8" Style="{StaticResource MaterialDesignRaisedButton}" Width="130" ToolTip="Confirm Quote and Continue" Opacity="1" Background="#FF263B8F" BorderBrush="#FF263B8F" Foreground="#FFFDD835" Margin="10,5" FontSize="18" Height="60"/>
                                <Button x:Name="btnDupCancel" Content="Cancel" Grid.Row="2" FontSize="18" Grid.Column="0" Click="CancelAddQuotes_click" Style="{StaticResource MaterialDesignRaisedButton}" Width="130" ToolTip="Cancel New Quote" Background="#FF263B8F" BorderBrush="#FF263B8F" Opacity="1" Foreground="#FFFDD835" Margin="10,5" Height="60"/>
                            </StackPanel>


                            <telerik:RadTabControl HorizontalAlignment="Stretch" Margin="155,28,9.333,9.668" VerticalAlignment="Stretch" Grid.ColumnSpan="4" >
                                <telerik:RadTabItem x:Name="quoteinfo" Visibility="Visible" Header="Main Info" Height="68" Width="202" MouseDown="Quoteinfo_MouseDown" FontSize="16" ScrollViewer.HorizontalScrollBarVisibility="Auto">
                                    <Grid x:Name="existingQuoteDataGrid" DataContext="{StaticResource quoteViewSource}" Margin="0,26,0,14" Visibility="Visible" Background="Transparent">
                                        <!-- Existing Quotes form -->

                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                            <RowDefinition Height="Auto"/>
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="558.5"/>
                                            <ColumnDefinition Width="41.5"/>
                                            <ColumnDefinition Width="600"/>
                                            <ColumnDefinition Width="600"/>
                                        </Grid.ColumnDefinitions>
                                        <Label Grid.Column="0" Grid.Row="0" FontSize="20" FontWeight="Bold" HorizontalContentAlignment="Center" Foreground="#FF263B8F" Content="Company Details" Grid.ColumnSpan="2" />
                                        <DatePicker x:Name="quotedate" materialDesign:HintAssist.Hint="Date" Grid.Column="0" Margin="122,5,140.5,6" Grid.Row="1" VerticalAlignment="Stretch" Width="296" Height="50" SelectedDate="{Binding Date1, Mode=TwoWay, NotifyOnValidationError=true, ValidatesOnExceptions=true}"/>



                                        <!-- NEED TO LINK SUBURB/PC TO COMBO -->
                                        <!-- Representitive = People / RepName = FullName -->

                                        <TextBox x:Name="rep" materialDesign:HintAssist.Hint="Representative" Grid.Column="0" Grid.Row="2" Margin="120,6,138.5,5" Text="{Binding Representative, Mode=TwoWay, NotifyOnValidationError=true, ValidatesOnExceptions=true}" Style="{StaticResource MaterialDesignFloatingHintTextBox}" Width="300" Height="50"/>
                                        <TextBox x:Name="custname" materialDesign:HintAssist.Hint="Company" Grid.Column="0" Grid.Row="3" Margin="120,3,138.5,3" Text="{Binding Company, Mode=TwoWay, NotifyOnValidationError=true, ValidatesOnExceptions=true}" Style="{StaticResource MaterialDesignFloatingHintTextBox}" Width="300" Height="45"/>
                                        <TextBox x:Name="custaddress" materialDesign:HintAssist.Hint="Customer Address" Margin="120,13,138.5,3" TextWrapping="Wrap" VerticalContentAlignment="Top" Width="300" Height="120" AcceptsReturn="True" Text="{Binding Address, Mode=TwoWay, NotifyOnValidationError=true, ValidatesOnExceptions=true}" Style="{StaticResource MaterialDesignFloatingHintTextBox}" VerticalScrollBarVisibility="Auto" SpellCheck.IsEnabled="True" Grid.Row="4" Grid.Column="0" Grid.RowSpan="2" HorizontalScrollBarVisibility="Auto"/>



                                        <!-- NEED TO AUTO FILL COMBINED STATE/PC FROM SUBURB ABOVE -->
                                        <TextBox x:Name="custcontact" materialDesign:HintAssist.Hint="Company Contact"  Grid.Column="0" Grid.Row="6" Text="{Binding ContactPerson, Mode=TwoWay, NotifyOnValidationError=true, ValidatesOnExceptions=true}" Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="120,2,138.5,3" Width="300" Height="50"/>
                                        <TextBox x:Name="custphone" materialDesign:HintAssist.Hint="Phone"  Grid.Column="0" Grid.Row="7" Text="{Binding Phone, Mode=TwoWay, NotifyOnValidationError=true, ValidatesOnExceptions=true}" Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="120,3,138.5,2" Width="300" Height="50"/>
                                        <TextBox x:Name="custmobile" materialDesign:HintAssist.Hint="Mobile"  Grid.Column="0" Grid.Row="8" Text="{Binding Mobile, Mode=TwoWay, NotifyOnValidationError=true, ValidatesOnExceptions=true}" Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="120,2,138.5,3" Width="300" Height="50"/>
                                        <TextBox x:Name="custemail" materialDesign:HintAssist.Hint="Email"  Grid.Column="0" Grid.Row="9" Text="{Binding Email, Mode=TwoWay, NotifyOnValidationError=true, ValidatesOnExceptions=true}" Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="120,3,138.5,2" Width="300" Height="50"/>
                                        <TextBox x:Name="custstrata" materialDesign:HintAssist.Hint="Cust Strata #" Grid.Column="0" Grid.Row="10" Text="{Binding StrataNo, Mode=TwoWay, NotifyOnValidationError=true, ValidatesOnExceptions=true}" Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="120,0,138.5,1" Width="300" Height="50"/>
                                        <TextBox x:Name="custref" materialDesign:HintAssist.Hint="Cust Ref #" Grid.Column="0" Grid.Row="11" Text="{Binding RefNo, Mode=TwoWay, NotifyOnValidationError=true, ValidatesOnExceptions=true}" Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="120,1,138.5,0" Width="300" Height="50"/>
                                        <!-- COLUMN 1 SITE DETAILS -->
                                        <Label Grid.Column="2" Grid.Row="0" FontSize="20" FontWeight="Bold" HorizontalContentAlignment="Center" Foreground="#FF263B8F" Content="Site Details" />
                                        <TextBox x:Name="siteaddress" materialDesign:HintAssist.Hint="Site Address" Margin="120,1,180,1" VerticalContentAlignment="Top" TextWrapping="Wrap" Width="300" AcceptsReturn="True" Text="{Binding JobLocation, Mode=TwoWay, NotifyOnValidationError=true, ValidatesOnExceptions=true}" Style="{StaticResource MaterialDesignFloatingHintTextBox}" VerticalScrollBarVisibility="Auto" SpellCheck.IsEnabled="True" Height="120" Grid.ColumnSpan="1" Grid.Row="1" Grid.Column="2" Grid.RowSpan="2"/>
                                        <TextBox x:Name="sitecontact" materialDesign:HintAssist.Hint="Site Contact"  Grid.Column="2" Grid.Row="3" Text="{Binding SiteContact, Mode=TwoWay, NotifyOnValidationError=true, ValidatesOnExceptions=true}" Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="120,1,180,0" Width="300" Height="50"/>
                                        <TextBox x:Name="sitephone" materialDesign:HintAssist.Hint="Phone"  Grid.Column="2" Grid.Row="4" Text="{Binding SCPhone, Mode=TwoWay, NotifyOnValidationError=true, ValidatesOnExceptions=true}" Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="120,8,180,8" Width="300" Height="50"/>
                                        <TextBox x:Name="sitemobile" materialDesign:HintAssist.Hint="Mobile"  Grid.Column="2" Grid.Row="5" Text="{Binding SCMobile, Mode=TwoWay, NotifyOnValidationError=true, ValidatesOnExceptions=true}" Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="120,10,180,10" Width="300" Height="50"/>
                                        <TextBox x:Name="siteemail" materialDesign:HintAssist.Hint="Email"  Grid.Column="2" Grid.Row="6" Text="{Binding SCEmail, Mode=TwoWay, NotifyOnValidationError=true, ValidatesOnExceptions=true}" Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="120,2,180,3" Width="300" Height="50"/>
                                        <!--   <CheckBox x:Name="swms" Style="{StaticResource MaterialDesignCheckBox}" IsChecked="{Binding SWMS, Mode=TwoWay, NotifyOnValidationError=true, ValidatesOnExceptions=true}" Grid.Column="2" Grid.Row="7" Margin="120,0,180,0" Content="SWMS Required?" Grid.RowSpan="3" Width="300"/> -->
                                        <CheckBox x:Name="swms" Style="{StaticResource MaterialDesignCheckBox}" IsChecked="{Binding SWMS, NotifyOnValidationError=true, TargetNullValue=False, ValidatesOnExceptions=true}" Grid.Column="2" Grid.Row="7" Margin="120,0,180,0" Content="SWMS Required?" Grid.RowSpan="3" Width="300"/>
                                        <CheckBox x:Name="induction" Style="{StaticResource MaterialDesignCheckBox}" IsChecked="{Binding Induction, NotifyOnValidationError=true, TargetNullValue=False, ValidatesOnExceptions=true}" Grid.Column="2" Grid.Row="9" Margin="120,0,180,0" Content="Induction Required?" Width="300"/>
                                        <TextBox x:Name="indtime" materialDesign:HintAssist.Hint="Induction Time"  Grid.Column="2" Grid.Row="10" Text="{Binding Inductiontime, NotifyOnValidationError=true, TargetNullValue=False, ValidatesOnExceptions=true}" Style="{StaticResource MaterialDesignFloatingHintTextBox}" Margin="120,0,180,1" Width="300" Height="50"/>
                                        <!-- COLUMN 2 PAYMENT DETAILS -->
                                        <Label Grid.Column="3" Grid.Row="0" FontSize="20" FontWeight="Bold" HorizontalContentAlignment="Center" Foreground="#FF263B8F" Content="Payment Conditions" Margin="0,0,138,0" />
                                        <CheckBox x:Name="pp" Style="{StaticResource MaterialDesignCheckBox}" IsChecked="{Binding PROGRESSPAYMENT, NotifyOnValidationError=true, TargetNullValue=False, ValidatesOnExceptions=true}" Grid.Column="3" Grid.Row="1" Margin="160,0,140,0" Content="Progress Payment" Width="300"/>
                                        <CheckBox x:Name="cod" Style="{StaticResource MaterialDesignCheckBox}" IsChecked="{Binding COD, NotifyOnValidationError=true, TargetNullValue=False, ValidatesOnExceptions=true}" Grid.Column="3" Grid.Row="2" Margin="160,0,140,0" Content="COD" Width="300"/>
                                        <CheckBox x:Name="svndays" Style="{StaticResource MaterialDesignCheckBox}" IsChecked="{Binding SvnDays, NotifyOnValidationError=true, TargetNullValue=False, ValidatesOnExceptions=true}" Grid.Column="3" Grid.Row="3" Margin="160,0,140,0" Content="7 Days" Width="300"/>
                                        <CheckBox x:Name="orderno" Style="{StaticResource MaterialDesignCheckBox}" IsChecked="{Binding OrderNo, NotifyOnValidationError=true, TargetNullValue=False, ValidatesOnExceptions=true}" Grid.Column="3" Grid.Row="4" Margin="160,0,140,0" Content="Order No" Width="300"/>
                                        <TextBox x:Name="notes" materialDesign:HintAssist.Hint="Internal Notes" Margin="25,2,70,8" TextWrapping="Wrap" VerticalAlignment="Stretch" AcceptsReturn="True" Text="{Binding Notes, Mode=TwoWay, NotifyOnValidationError=true, ValidatesOnExceptions=true}" Style="{StaticResource MaterialDesignFloatingHintTextBox}" 
